{
  "name": "vfs-app",
  "version": "0.2.3",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "npx vue-cli-service build --no-module --skip-plugins pwa,workbox"
  },
  "dependencies": {
    "@bit/sistemium.vue.mixins.debug": "^0.0.1",
    "@bit/sistemium.vue.resize": "^0.0.4",
    "@bit/sistemium.vue.services.native": "^0.0.1",
    "axios": "^0.27.2",
    "dayjs": "^1.11.3",
    "element-ui": "^2.15.12",
    "fast-sort": "^3.2.0",
    "lodash": "^4.17.21",
    "mint-ui": "^2.2.13",
    "qs": "^6.11.0",
    "sistemium-data-vue": "^0.2.7",
    "sistemium-dates": "^1.1.0",
    "sistemium-debug": "^1.0.1",
    "uuid": "latest",
    "vue": "^2.6.14",
    "vue-i18n": "^8.26.3",
    "vue-router": "^3.5.4",
    "vue-router-multiguard": "^1.0.3",
    "vue2-google-maps": "^0.10.7",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/eslint-parser": "^7.18.2",
    "@intlify/vue-i18n-loader": "^1.1.0",
    "@vue/cli-plugin-babel": "~5.0.6",
    "@vue/cli-plugin-eslint": "~5.0.6",
    "@vue/cli-plugin-router": "~5.0.6",
    "@vue/cli-plugin-unit-mocha": "~5.0.6",
    "@vue/cli-plugin-vuex": "~5.0.6",
    "@vue/cli-service": "~5.0.6",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/test-utils": "^1.3.0",
    "chai": "^4.3.6",
    "chalk": "^4.1.2",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^8.7.1",
    "eslint-plugin-vuejs-accessibility": "^1.2.0",
    "sass": "^1.52.3",
    "sass-loader": "^12.6.0",
    "vue-cli-plugin-pug": "~2.0.0",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "padded-blocks": "off",
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "no-use-before-define": [
        "error",
        {
          "functions": false,
          "classes": true
        }
      ],
      "vue/multi-word-component-names": "off",
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 2
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "bit": {
    "env": {},
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
